<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>USER</title>
  </head>
  <body>
    <a href="/account/add" class="btn btn-default">Add New User</a>
    <table border="1">
      <thead>
        <tr>
          <th>No</th>
          <th>Nama</th>
          <th>Email</th>
          <th>Phone</th>
          <th>Address</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        <% data_admins.forEach((da, index)=>{%>
        <tr>
          <td><%= index+1 %></td>
          <td><%= da.name %></td>
          <td><%= da.email %></td>
          <td><%= da.phone %></td>
          <td><%= da.address %></td>
          <td>
            <a href="/account/admin/edit/<%= da.id %>">Edit</a>
            <a href="/account/admin/<%= da.id %>">Delete</a></td>
        </tr>
        <%})%>
      </tbody>
    </table>
  </body>
</html>
